<tr class='route_row' data-helper='path'>
  <td class="helper" data-route-name='<%= route[:name] %>'>
    <% if route[:name].present? %>
      <%= route[:name] %><span class='helper'>_path</span>
    <% end %>
  </td>
  <td class="verb" data-route-verb='<%= route[:verb].downcase %>'>
    <% if route[:verb] && route[:verb] != '' %>
      <span><%= route[:verb] %></span>
    <% end %>
  </td>
  <td class="path" data-route-path='<%= route[:path] %>'>
    <%= route[:path] %>
  </td>
  <td class="controller" data-route-reqs='<%= route[:reqs] %>'>
    <%= route[:reqs] %>
  </td>
</tr>
